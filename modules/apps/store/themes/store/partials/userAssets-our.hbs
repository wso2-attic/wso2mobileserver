<div id="myasset-container" class="row">
	<div class="span9">
		{{#if userAssets}}
		{{#itr userAssets}}
		<h3 class="asset-title-separator asset-type-{{key}}">{{cap key}}s</span><div class="pagination own-asset-slideshow-pag" id="own-asset-slideshow-pag-{{key}}"></div></h3>
		<div class="top-asset-slideshow-cont">
			<div class="html_carousel">
				<div id="asset-in-{{key}}">

					{{#each value}}
					{{#attributes}}
					<!--div class="row-fluid"-->
					<div class="store-my-item jtootip">

						<!-- Modal Devices -->
						<div id="devicesList-{{../id}}" class="modal hide" tabindex="-1" role="dialog" aria-hidden="true" style="width:700px">

							<div class="modal-header">
								<button type="button" class="close" data-dismiss="modal"
								aria-hidden="true">
									x
								</button>
								<h4 id="myModalLabel">Select your device to uninstall</h4>
							</div>
							<div class="modal-body">

								{{#../../../devices}}
								<div data-dismiss="modal"  class="span1 text-center device-image-block-modal  asset-remove-btn" style="text-align: center; cursor: pointer"
								data-device-id="{{id}}" type="{{../../../key}}" data-aid="{{../../id}}">
									<img class="device-image-modal" style="height: 100px" data-device-id="{{id}}" 
									src="/assets/wso2mobile/img/models/{{model}}.png">
									<div class="text-center" style="font-size: x-small">
										{{name}}
									</div>
								</div>
								{{/../../../devices}}

							</div>
						</div>
						<!-- Modal Devices -->

						<div class="tooltip_description row" style="display:none" title="{{firstName}} {{lastName}}'s Devices">
							jtooltip
						</div>

						<div class="store-my-item-thumbnail">
							<a href="{{url "/asset/"}}{{../../key}}/{{../id}}"><img src="{{images_thumbnail}}"></a>
						</div>
						<div class="store-my-item-info">
							<a class="asset-title" href="{{url "/asset/"}}{{../../key}}/{{../id}}"> <strong>{{overview_name}}</strong> </a>
							<small class="bookmark-assert-provider">by {{overview_provider}}</small>
							<br>
							<div class="btn-group">
								<a href="#devicesList-{{../id}}" data-toggle="modal" class="btn btn-mini" type="{{../../key}}" data-aid="{{../id}}" data-toggle="tooltip" title="{{t "Remove"}} {{../../key}}"> <i class="icon-remove"></i> </a>
							</div>
						</div>

					</div>
					<!--/div-->
					{{/attributes}}
					{{/each}}
				</div>
			</div>
		</div>
		<hr>
		{{/itr}}
		{{else}}
		<div id="no-data-my-items" class="no-data">
			<i class="icon-inbox"></i>
			<p>
				{{t "You still haven't any bookmarks from the Store."}}
			</p>
			<p>
				<a href="../store/assets/gadget/" class="btn btn-primary">{{t "Bookmark now"}}</a>
			</p>
		</div>
		{{/if}}
	</div>
</div>

